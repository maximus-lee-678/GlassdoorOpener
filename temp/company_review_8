                }
            }
            if (startIndex && !endIndex) {
                result.params[key] = { ...result.params[key], id: startIndex };
            }
        });
        const jobTitleParam = jobTitleRegex.exec(url);
        if (jobTitleParam && jobTitleParam.length > 1 && jobTitleParam[1].indexOf('<') < 0) {
            const filterValue = getHumanReadableText(jobTitleParam[1]);
            result.jobTitleFilter = filterValue;
        }
        if (Object.keys(result.params).length === 0 && !result.jobTitleFilter) {
            const urlRegexForEmployerName = /(?<employer>.*)-(?:Reviews|Evaluaciones|Avis|Avaliações|Avalia%C3%A7%C3%B5es|Bewertungen|Opiniones|Recensioni)-E(?<employerId>\d+)/;
            // eslint-disable-next-line no-unused-vars
            const [path, employer, employerId] = urlRegexForEmployerName.exec(urlParamsArr);
            result.params.path = { employer, employerId };
            result.pageSegment = 'e';
        }
        // eslint-disable-next-line no-empty
    }
    catch (e) { }
    return result;
